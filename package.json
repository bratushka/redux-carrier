{
  "name": "redux-carrier",
  "version": "0.1.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "description": "Redux state manager for handling requests",
  "scripts": {
    "lint": "eslint ./src/ ./test/",
    "test": "mocha --compilers js:babel-core/register",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -i src/index.js -o dist/index.js -f umd",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -i src/index.js -o dist/index.min.js -f umd",
    "build": "yarn run build:commonjs && yarn run build:es"
  },
  "author": "Vasyl Bratushka <bratushka.v@gmail.com>",
  "license": "WTFPL",
  "keywords": [
    "carrier",
    "immutable",
    "Promise",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bratushka/redux-carrier.git"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "immutable": "^3.8.1",
    "mocha": "^3.5.0",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "lodash.values": "^4.3.0"
  }
}
